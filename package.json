{
  "name": "Sermonindex-GUI",
  "productName": "SermonindexGUI",
  "version": "1.1.0",
  "description": "Sermon downloader is offered by sermonindex.net. This is a free to use and distribute. The purpose is to provide sermons download in a batch with single click capabilities to the user. It is made available for God's glory. Enjoy and rejoice inthe Lord! IF you liked it please feel free to send a note of encouragement to sermonindex.",
  "main": "js/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package-linux": "electron-packager . SermonIndex-GUI --overwrite --asar --platform=linux --arch=x64 --icon=images/sermonindex-logo.png --out=release-builds",
    "create-debian-installer": "electron-installer-debian --overwrite --src release-builds/SermonIndex-GUI-linux-x64/ --arch amd64 --icon=images/sermonindex-logo.png --config debian.json",
    "package-mac": "electron-packager . SermonIndex-GUI --overwrite --platform=darwin --arch=x64 --out=release-builds --icon=images/sermonindex-logo.icns",
    "create-mac-installer": "electron-installer-dmg ./release-builds/SermonIndex-GUI-darwin-x64/SermonIndex-GUI.app SermonIndex-GUI --out=release-builds --overwrite --icon=images/sermonindex-logo.icns",
    "package-win": "electron-packager . SermonIndex-GUI --overwrite --asar --icon=images/sermonindex-logo.ico --out=release-builds"
  },
  "keywords": [
    "sermondownloader",
    "sermonindex"
  ],
  "author": "Vincent L",
  "license": "ISC",
  "devDependencies": {
    "download": "^8.0.0",
    "electron": "^26.2.1",
    "electron-log": "^4.1.2",
    "electron-packager": "^17.1.2",
    "electron-winstaller": "^5.0.0",
    "jquery": "^3.5.0"
  },
  "dependencies": {
    "electron-log": "^4.1.2",
    "jquery": "^3.5.0",
    "needle": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VincentXE/SermonIndex-GUI"
  },
  "bugs": {
    "url": "https://github.com/VincentXE/SermonIndex-GUI/issues"
  },
  "homepage": "https://github.com/VincentXE/SermonIndex-GUI#readme"
}
